    $(".geojson_links").click(function (e) {
      console.warn($(e).data("url"));
      var url = $(e.target).data("url");
      var random_color = getRandomColor();
      if (layers[url] != undefined) {
        random_color = "#666666";
        $(e.target).find(".pull-right div").css("background-color", "#FFFFFF");
      }
      $(e.target).find(".circle").addClass('spinner');
      $('head').append('<style>button#'+ e.target.id +' .circle.spinner:not(:required):before{border-top-color:'+random_color+';}</style>');
      $('head').append('<style>button#'+ e.target.id +' .circle.spinner:not(:required):before{border-left-color:'+random_color+';}</style>');

      $.getJSON('http://www.whateverorigin.org/get?url=' + url + '&callback=?',
        function (data) {
          var response;
          if (typeof data.contents == "string") {
            response = $.parseJSON(data.contents);
          }
          else {
            response = data.contents;
          }
          if (layers[url] == undefined) {
            var new_layer = L.mapbox.featureLayer();
            for (var i = 0; i < response.features.length; i++) {
              response.features[i].properties['marker-color'] = random_color;
              response.features[i].properties['stroke'] = random_color;
              response.features[i].properties['fill'] = random_color;
              response.features[i].properties['fill-opacity'] = 0.3;
              response.features[i].properties['stroke-width'] = 3;
            }
            new_layer.setGeoJSON(response);
            new_layer.eachLayer(function(layer) {

                // here you call `bindPopup` with a string of HTML you create - the feature
                // properties declared above are available under `layer.feature.properties`
                //var content = '<h2>A ferry ride!<\/h2>' +
                //    '<p>From: ' + layer.feature.properties.from + '<br \/>' +
                //    'to: ' + layer.feature.properties.to + '<\/p>';
                var tooltip_content = '<table class="table table-bordered table-striped table-hover table-sm">';
                var properties = layer.feature.properties;
                for (var key in properties) {
                  if (properties.hasOwnProperty(key) && !(['marker-color', 'stroke', 'fill', 'fill-opacity', 'stroke-width'].indexOf(key) >= 0)) {
                    tooltip_content += "<tr>";
                    tooltip_content += "<td>" + key + "</td>" + "<td>" + properties[key] + "</td>";
                    tooltip_content += "</tr>";
                  }
                }
                tooltip_content += '</table>';
                layer.bindPopup(tooltip_content);
            });
            var clusterGroup = new L.MarkerClusterGroup({
                  // This option sets custom options for the L.Polygon that's drawn when
                  // the user hovers over the icon for a marker cluster. The options
                  // given here are taken from the L.Path options:
                  // http://leafletjs.com/reference.html#path
                  polygonOptions: {
                    fillColor: random_color,
                    color: random_color,
                    weight: 2,
                    opacity: 1,
                    fillOpacity: 0.5
                  },

                  iconCreateFunction: function(cluster) {
                    return new L.DivIcon({
                              iconSize: [32, 32],
                              className: 'css-icon',
                              html: '<div style="text-align:center; color:#FFFFFF; padding:6px 0px; border-radius:16px; background-color:' +
                              random_color + '">' + cluster.getChildCount() + '</div>'
                            });
                    //return L.mapbox.marker.icon({
                    //  // show the number of markers in the cluster on the icon.
                    //  'marker-symbol': cluster.getChildCount() > 99 ? 'circle' : cluster.getChildCount(),
                    //  'marker-color': random_color,
                    //  'marker-size': cluster.getChildCount() > 99 ? 'large' : 'medium'
                    //});
                  }
            });
            new_layer.eachLayer(function(layer) {
              clusterGroup.addLayer(layer);
            });
            map.addLayer(clusterGroup);
            //map.addLayer(new_layer);
            insert_layer(url, clusterGroup, random_color, e);
          }
          else {
            map.removeLayer(layers[url]);
            delete_layer(url, e);
          }
          $(e.target).find(".circle").removeClass('spinner');
          //If the expected response is JSON
          //var response = $.parseJSON(data.contents);
      });
    });

    function insert_layer(url, layer, random_color, e) {
      layers[url] = layer;
      $(e.target).find(".pull-right div").css("background-color", random_color);
    }

    function delete_layer(url, e) {
